    "dump:prod": "a0deploy export --config_file=configs/config-prod.json --format=directory --output_folder=exported",
    "deploy:staging": "a0deploy import --config_file=configs/config-staging.json --input_file=src",
    "deploy:prod": "a0deploy import --config_file=configs/config-prod.json --input_file=src"
  },
